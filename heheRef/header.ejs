<script>
// filepath: c:\Users\njbsa\Desktop\shoppingmall\wait.html
// Sidebar open/close logic
const sidebarBtn = document.querySelector('.eNavFold');
const sidebar = document.getElementById('sidebarNav');
const sidebarOverlay = document.getElementById('sidebarOverlay');
const sidebarCloseBtn = document.getElementById('sidebarCloseBtn');
const sidebarCategories = document.querySelectorAll('.sidebar-category');
const sidebarSubmenus = document.querySelectorAll('.sidebar-submenu');


// Open sidebar
sidebarBtn && sidebarBtn.addEventListener('click', () => {
    sidebar.classList.add('active');
    sidebarOverlay.classList.add('active');
    document.body.style.overflow = 'hidden';
});

sidebarBtn && sidebarBtn.addEventListener('hover', () => {
    sidebar.classList.add('active');
    sidebarOverlay.classList.add('active');
    document.body.style.overflow = 'hidden';
});

// Close sidebar
function closeSidebar() {
    sidebar.classList.remove('active');
    sidebarOverlay.classList.remove('active');
    document.body.style.overflow = '';
}
sidebarCloseBtn && sidebarCloseBtn.addEventListener('click', closeSidebar);
sidebarOverlay && sidebarOverlay.addEventListener('click', closeSidebar);

// Sidebar submenu toggle
sidebarCategories.forEach(btn => {
    btn.addEventListener('click', function() {
        const cat = btn.getAttribute('data-sidebar-cat');
        const submenu = document.querySelector(`.sidebar-submenu[data-sidebar-sub="${cat}"]`);
        // If no submenu, just close sidebar and go to link if needed
        if (!submenu) {
            closeSidebar();
            // Optionally, navigate if needed
            // window.location.href = btn.getAttribute('href');
            return;
        }
        // Toggle submenu
        const isActive = submenu.classList.contains('active');
        // Close all submenus
        sidebarSubmenus.forEach(sm => sm.classList.remove('active'));
        sidebarCategories.forEach(b => b.classList.remove('active'));
        if (!isActive) {
            submenu.classList.add('active');
            btn.classList.add('active');
        }
    });
});
</script>